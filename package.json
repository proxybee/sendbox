{
  "name": "sendbox",
  "version": "1.0.0",
  "description": "a dispath application",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "start:dev": "nodemon --ignore ./public --exec ./node_modules/.bin/babel-node ./src/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "debug": "set DEBUG=index & npm run start:dev",
    "lint": "eslint ./src/index.js",
    "createTable": "babel-node ./src/models/createTable.js --exit",
    "dropTable": "babel-node ./src/models/dropTable.js --exit"
  },
  "keywords": [
    "dispatch",
    "errand",
    "parcel",
    "item",
    "large",
    "medium",
    "small",
    "document",
    "letter",
    "package",
    "weight",
    "size",
    "length"
  ],
  "author": "Bitech",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@types/uuid": "^8.3.4",
    "bcrypt": "^5.0.1",
    "chalk": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "postgresql": "^0.0.1",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "babel-loader": "^8.2.3",
    "babel-preset-airbnb": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  }
}
